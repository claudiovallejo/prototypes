---
layout: main4
title: Filipao Nunes
---
<div class="mw8 center ph4 ph8-m ph0-l">
  <%# Caption %>
  <div class="mt10 mb8 mv6-ns ph0-l">
    <h1 class="mw6 ma0 roboto fw4 f3 f2-ns lh-title">We're a studio that lorem ipsum dolor sit amet.</h1>
  </div>
  <%# Featured Projects %>
  <% l = data.projects3.length %>
  <% data.projects3.each_with_index do |project, r| %>
    <% aspect_ratio = project.images.card.aspect_ratio %>
    <% align = project.images.card.align %>
    <% if project.featured %>
      <% if aspect_ratio === "1x1" || aspect_ratio === "6x4" %>
        <a class="db dim no-underline <% if r != l - 1 %>mb8<% else %>mb10<% end %>" href="/prototype4/projects/<%= purl project.name %>">
          <div class="aspect-ratio aspect-ratio--<%= aspect_ratio %>">
            <div class="aspect-ratio--object bg-near-white"></div>
          </div>
          <p class="mt2 mb0 roboto fw3 lh-title dark-gray">
            <%= project.name %><% if r % 2 == 0 %>  \  <% else %>  /  <% end %><span class="silver"><%= project.category %></span>
          </p>
        </a>
      <% else %>
        <div class="cf">
          <a class="db <% if align === "right" %>fr<% else %>fl<% end %> w-two-thirds dim no-underline <% if r != l - 1 %>mb8<% else %>mb10<% end %>" href="/prototype4/projects/<%= purl project.name %>">
            <div class="aspect-ratio aspect-ratio--<%= aspect_ratio %>">
              <div class="aspect-ratio--object bg-near-white"></div>
            </div>
            <p class="mt2 mb0 roboto fw3 lh-title dark-gray">
              <%= project.name %><% if r % 2 == 0 %>  \  <% else %>  /  <% end %><span class="silver"><%= project.category %></span>
            </p>
          </a>
        </div>
      <% end %>
    <% end %>
  <% end %>
</div>
