---
layout: main3
title: Filipao Nunes
---
<div class="mw8 center ph4 ph8-m ph0-l">
  <%# Caption %>
  <div class="mv8 mv6-ns ph0-l tc">
    <h1 class="mw6 center ma0 roboto fw4 f3 f2-ns lh-title">We're a studio that lorem ipsum dolor sit amet.</h1>
  </div>
  <%# Featured Projects %>
  <% l = data.projects3.length %>
  <% data.projects3.each_with_index do |project, r| %>
    <% aspect_ratio = project.images.card.aspect_ratio %>
    <% align = project.images.card.align %>
    <% if project.featured %>
      <% if aspect_ratio === "1x1" || aspect_ratio === "6x4" %>
        <a class="db link dim <% if r != l - 1 %>mb8<% end %>" href="/prototype3/projects/<%= purl project.name %>">
          <div class="aspect-ratio aspect-ratio--<%= aspect_ratio %>">
            <div class="aspect-ratio--object bg-near-white"></div>
          </div>
          <p class="mt2 mb0 roboto fw3 lh-title black"><%= project.name %>, <span class="silver"><%= project.category %></span></p>
        </a>
      <% else %>
        <div class="cf">
          <a class="db <% if align === "right" %>fr<% else %>fl<% end %> w-two-thirds link dim <% if r != l - 1 %>mb8<% end %>" href="/prototype3/projects/<%= purl project.name %>">
            <div class="aspect-ratio aspect-ratio--<%= aspect_ratio %>">
              <div class="aspect-ratio--object bg-near-white"></div>
            </div>
            <p class="mt2 mb0 roboto fw3 lh-title black"><%= project.name %>, <span class="silver"><%= project.category %></span></p>
          </a>
        </div>
      <% end %>
    <% end %>
  <% end %>
</div>
